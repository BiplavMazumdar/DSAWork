import { useEffect, useState } from "react";
import "./styles.css";


export default function App() {
  const [ count , setCount ] = useState(0);
useEffect ( ()=> {
  let interval = setInterval ( ()=> {
    setCount ( count + 1 ) ;
 } , 1000 ) ;
  return ()=> {
    clearInterval ( interval ) ;
 } ;
} ) ;
  return (
    <div className="App">
      <h1>{count}</h1>
      <h2>{count/2}</h2>
    </div>
  );
}



